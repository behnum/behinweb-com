{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.content.publicFolder": {
    "path": "src/assets",
    "relative": true
  },
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/content/blog",
      "previewPath": "'posts'"
    },
    {
      "title": "pages",
      "path": "[[workspace]]/src/pages"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": "'posts'",
      "filePrefix": null,
      "clearEmpty": true,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "pubDatetime",
          "name": "pubDatetime",
          "type": "datetime"
        },
        {
          "title": "featured",
          "name": "featured",
          "type": "boolean"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.startCommand": "bun dev"
}