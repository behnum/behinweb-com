    "fuse.js": "^7.1.0",
    "github-slugger": "^2.0.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.10.14",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@astrojs/react": "4.4.2",
    "@astrojs/sitemap": "3.7.0",
    "@astrojs/tailwind": "6.0.2",
    "@divriots/jampack": "^0.23.4",
